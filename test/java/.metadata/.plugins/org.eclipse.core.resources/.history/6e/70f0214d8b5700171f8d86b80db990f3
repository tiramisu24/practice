package demo1;

class ThreadDemo1 extends Thread{
	
	public void run(){
		for(int =0; i<10; i++){
			System.out.println(i);
			Thread.sleep(100);
			
		}
	}
}
public class test1 {
	public static void main(String[] args){
		 ThreadDemo1 td1 = new Runner();
		 td1.start();
		 ThreadDemo1 td2 = new Runner();
		 td2.start();
	}
}
